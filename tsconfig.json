{
  "compilerOptions": {
    "target": "ES2018",
    "module": "NodeNext",
    "lib": ["DOM", "DOM.Iterable", "ES2018.AsyncIterable", "ES2018"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "noEmit": true  
  }
}
